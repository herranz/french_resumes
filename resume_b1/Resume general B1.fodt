   <text:p text:style-name="P111"><text:span text:style-name="T109">Toujours </text:span>on utilise l&apos;auxiliai<text:span text:style-name="T109">re </text:span><text:span text:style-name="T117">Être</text:span><text:span text:style-name="T109"> pour conjuguer </text:span><text:span text:style-name="T120">p</text:span><text:span text:style-name="T118">lus-que-parfait</text:span><text:span text:style-name="T109">. </text:span></text:p>
