     <table:table-cell table:style-name="Tabla24.A1" office:value-type="string">
      <text:p text:style-name="P66">Positif</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla24.B1" table:number-columns-spanned="2" office:value-type="string">
      <text:p text:style-name="P66">Négatif</text:p>
     </table:table-cell>
     <table:covered-table-cell/>
    </table:table-row>
    <table:table-row table:style-name="Tabla24.2">
     <table:table-cell table:style-name="Tabla24.A2" office:value-type="string">
