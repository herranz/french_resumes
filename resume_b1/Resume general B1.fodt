     <table:table-cell table:style-name="Tabla28.A2" office:value-type="string">
      <text:p text:style-name="P204">Passé Composé</text:p>
     </table:table-cell>
     <table:table-cell table:style-name="Tabla28.A2" office:value-type="string">
